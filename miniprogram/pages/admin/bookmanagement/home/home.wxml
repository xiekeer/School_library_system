<!--pages/admin/bookmanagement/home/home.wxml-->

<view class="cu-list grid col-3 gridBorder margin-top">
    <view class="cu-item" wx:for="{{iconList}}" wx:if="{{index<gridCol*3}}">
      <view class="cuIcon-{{item.icon}} text-{{item.color}}" bindtap="IconTap" data-menuurl="{{item.menuurl}}">
      </view>
      <text>{{item.name}}</text>
    </view>
  </view>
